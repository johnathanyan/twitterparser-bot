{
	"manifest_version": 2,

	"name": "EastBay Twitter Bot",
	"description": "Specially designed for Crips.",

	"version": "1.0",

	"content_scripts": [
		{
      "matches": ["http://eastbay.com/product/*"],
      "js": ["eastbaya2c.js"]
    	}
	],

	"permissions": [
		"https://store.nike.com/*",
		"https://twitter.com/*",
		"tabs"
	]

